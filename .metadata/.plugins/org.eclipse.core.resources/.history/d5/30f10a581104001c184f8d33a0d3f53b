
public class RecursiveBubbleSort {

	public static void main(String[] args) {
		// TODO Auto-generated method stub
		
		int arr[] = {2,5,7,1,5,6};
		RBS(arr);
		
	}
	
    public static void RBS(int arr[])
    {
    	
    	int n = arr.length;
    	
    	for(int i = 0;i<n-1;i++)
		{
			System.out.println(arr[i]);
		}
    }

}
